{
    "name": "Gerschtli/time-manager",
    "require": {
        "php"         : ">=5.6",
        "slim/slim"   : "^2@stable",
        "doctrine/orm": "@stable"
    },
    "require-dev": {
        "codeclimate/php-test-reporter": "dev-master",
        "friendsofphp/php-cs-fixer"    : "@stable",
        "phpmd/phpmd"                  : "@stable",
        "phpunit/phpunit"              : "@stable",
        "squizlabs/PHP_CodeSniffer"    : "@stable"
    },
    "autoload": {
        "psr-4": {
            "TimeManager\\": "lib/TimeManager/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
